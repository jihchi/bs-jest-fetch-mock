{
  "name": "bs-jest-fetch-mock",
  "version": "0.4.0",
  "description": "jest-fetch-mock bindings for BuckleScript in Reason",
  "repository": "https://github.com/jihchi/bs-jest-fetch-mock.git",
  "scripts": {
    "build": "bsb -make-world",
    "clean": "bsb -clean-world",
    "coveralls": "npm test -- --coverage && coveralls < coverage/lcov.info",
    "prepublishOnly": "npm run build",
    "start": "bsb -make-world -w",
    "test": "bsb -make-world && jest"
  },
  "keywords": [
    "BuckleScript",
    "jest-fetch-mock"
  ],
  "author": "Jihchi Lee <achi@987.tw>",
  "license": "MIT",
  "files": [
    "/src/**/*.re"
  ],
  "dependencies": {},
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.9",
    "bs-fetch": "^0.5.0",
    "bs-platform": "^7.0.1",
    "coveralls": "^3.0.9",
    "jest-fetch-mock": "^3.0.0"
  },
  "peerDependencies": {
    "jest-fetch-mock": "^3.0.0"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "<rootDir>/setup.jest.js"
    ]
  }
}
