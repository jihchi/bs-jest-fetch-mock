{
  "name": "bs-jest-fetch-mock",
  "version": "1.0.0",
  "namespace": true,
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "sources": [
    { "dir": "src" },
    { "dir": "__tests__", "type": "dev" }
  ],
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bisect_ppx",
    "bs-fetch"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "ppx-flags": [
    ["bisect_ppx/ppx", "--exclude-files", ".*\\/__tests__\\/.*\\.re$$"]
  ],
  "warnings": {
    "error": "+101"
  },
  "refmt": 3
}
